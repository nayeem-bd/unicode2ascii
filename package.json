    "test": "node test/unicode2ascii.test.js"
  },
  "keywords": [
    "unicode",
    "bangla",
    "bijoy",
    "bengali"
  ],
  "author": "Md. Nimuzzaman",
  "license": "MIT",
  "devDependencies": {
    "vite": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nayeem-bd/unicode2ascii.git"
  }
